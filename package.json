{
  "name": "@inaiat/fastify-papr",
  "version": "0.0.5",
  "description": "Fastify Papr Plugin Integration",
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "exports": "./dist/index.js",
	"types": "./dist/index.d.ts",
  "scripts": {
    "test": "ava",
    "build": "rimraf ./dist && mkdir dist && tsc --outDir dist && git rev-parse HEAD > BUILD_SHA",
    "lint": "eslint . --fix",
    "lint:fix": "prettier --loglevel warn --write \"src/**/*.{ts,tsx}\" && eslint \"*/**/*.{js,ts,tsx}\" --quiet --fix",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inaiat/fastify-papr.git"
  },
  "keywords": [
    "fastify",
    "mongodb",
    "papr"
  ],
  "contributors": [
    "Inaiat <inaiat@gmail.com>"
  ],
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/MIT"
    }
  ],
  "homepage": "https://github.com/inaiat/fastify-papr.git",
  "dependencies": {
    "fastify-plugin": "^4.1.0",
    "papr": "^7.2.1"
  },
  "peerDependencies": {
    "fastify": "^4.3.0",
    "mongodb": "^4.8.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.224",
    "@swc/helpers": "^0.4.3",
    "@types/node": "18.6.4",
    "@types/semver": "^7.3.10",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "ava": "^4.3.1",
    "c8": "^7.12.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fastify": "^4.3.0",
    "mongodb-memory-server": "^8.8.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "semver": "^7.3.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "files": [
    "dist"
  ],
  "directories": {
    "test": "test"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "config": {
    "mongodbMemoryServer": {
      "debug": "0"
    }
  },
  "author": "inaiat@gmail.com"
}
